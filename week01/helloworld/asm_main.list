     1                                  ;**************************************
     2                                  ; print out hello world
     3                                  ;***************************************
     4                                  
     5                                  section .data
     6 00000000 48656C6C6F20576F72-           hello: db 'Hello World!', 10
     7 00000009 6C64210A           
     8                                        helloLen: equ $-hello
     9                                  
    10                                  section .text
    11                                        global asm_main
    12                                  
    13                                  asm_main:             
    14 00000000 B804000000                    mov eax,4           ; 4 is the vector for the syswrite function
    15 00000005 BB01000000                    mov ebx,1           ; standard output, 1 is for stdout
    16 0000000A B9[00000000]                  mov ecx,hello       ; the address of the string
    17 0000000F BA0D000000                    mov edx,helloLen    ; size of the string (number of characters)
    18 00000014 CD80                          int 80h             ; call the interrupt to execute the syswrite function
    19                                  
    20                                  end_start:
    21 00000016 B801000000                    mov eax,1           ; vector for the exit function
    22 0000001B BB00000000                    mov ebx,0           ; return code 0
    23 00000020 CD80                          int 80h             ; call the interrupt to execute the exit function
